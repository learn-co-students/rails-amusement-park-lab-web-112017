<% if flash[:msg] %>
  <p><%= flash[:msg][0] %><p>
<% end %>


<p>Name: <%= @user.name %></p>
<p>Mood: <%= @user.mood %></p>
<p>Tickets: <%= @user.tickets %></p>
<p>Height: <%= @user.height %></p>
<% if @user.admin %>
